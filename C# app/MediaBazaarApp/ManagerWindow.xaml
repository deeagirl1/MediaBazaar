                                <Label Content="Choose a department to see its statistics" Foreground="White" HorizontalAlignment="Left" Margin="224,10,0,0" VerticalAlignment="Top" FontSize="24" FontWeight="Bold" Grid.Column="2" Height="49" Width="466"/>
                                <Label Content="For a selected time period:" Foreground="White" HorizontalAlignment="Left" Margin="619.328,86,0,0" VerticalAlignment="Top" FontSize="18" Grid.Column="2" Height="40" Width="217"/>
                                <DatePicker x:Name="dpStartDateDepartment" HorizontalAlignment="Left" Margin="510.328,132,0,0" VerticalAlignment="Top" Grid.Column="2" Height="28" Width="180"/>
                                <DatePicker x:Name="dpEndDateDepartment" HorizontalAlignment="Left" Margin="769.328,132,0,0" VerticalAlignment="Top" Grid.Column="2" Height="28" Width="174"/>
                                <Button x:Name="btnGetInfoDepartments" Content="Display" HorizontalAlignment="Left" Margin="665.328,221,0,0" VerticalAlignment="Top" Width="155" Height="51" Grid.Column="2" Click="btnGetInfoDepartments_Click"/>
                                <ComboBox x:Name="cmbDepartments1" HorizontalAlignment="Left" Margin="510.328,169,0,0" VerticalAlignment="Top" Width="433" Grid.Column="2" Height="25"/>
                                <Label x:Name="lbSalesPerDepartment" Content="Label" Grid.Column="2" Foreground="White" HorizontalAlignment="Left" Margin="888.328,347,0,0" VerticalAlignment="Top" FontSize="16" RenderTransformOrigin="2.16,0.661" Height="36" Width="55"/>
                                <Label x:Name="lbTotalSalariesPaidPerDepartment" Foreground="White" Content="Label" Grid.Column="2" HorizontalAlignment="Left" Margin="888.328,388,0,0" VerticalAlignment="Top" FontSize="16" RenderTransformOrigin="2.16,0.661" Height="36" Width="55"/>
                                <Label Content="Total salaries paid per department" Foreground="White" HorizontalAlignment="Left" Margin="547.328,388,0,0" VerticalAlignment="Top" FontSize="16" FontWeight="Bold" Grid.Column="2" Height="36" Width="273"/>
                                <Label Content="Average salary per department" Foreground="White" HorizontalAlignment="Left" Margin="87,347,0,0" VerticalAlignment="Top" FontSize="16" FontWeight="Bold" Grid.Column="1" Height="36" Width="273" Grid.ColumnSpan="2"/>
                                <Label Content="People per department" Foreground="White" HorizontalAlignment="Left" Margin="87,388,0,0" VerticalAlignment="Top" FontSize="16" FontWeight="Bold" Grid.Column="1" Height="36" Width="273" Grid.ColumnSpan="2"/>
