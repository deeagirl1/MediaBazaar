                                <Label Content="For a selected time period:" Foreground="White" HorizontalAlignment="Left" Margin="738,23,0,0" VerticalAlignment="Top" FontSize="18" Grid.Column="1" Height="38" Width="258"/>
                                <Label Content="Total salary paid to employees for period" Foreground="White" HorizontalAlignment="Left" Margin="605,336,0,0" VerticalAlignment="Top" FontSize="16" FontWeight="Bold" Grid.Column="1" Height="35" Width="373"/>
                                <Label x:Name="lblTotalSalaryPaidForPeriod" Content="Label" Foreground="White" HorizontalAlignment="Left" Margin="1027,336,0,0" VerticalAlignment="Top" FontSize="16" RenderTransformOrigin="1.904,0.613" Grid.Column="1" Height="35" Width="56"/>
                                <Label Content="Average hourly wage for period" HorizontalAlignment="Left" Foreground="White" Margin="605,408,0,0" VerticalAlignment="Top" FontSize="16" FontWeight="Bold" Grid.Column="1" Height="35" Width="291"/>
                                <Label x:Name="lblAverageSalaryForPeriod" Content="Label" Foreground="White" HorizontalAlignment="Left" Margin="1027,408,0,0" VerticalAlignment="Top" FontSize="16" RenderTransformOrigin="1.904,0.613" Grid.Column="1" Height="35" Width="56"/>
                                <Label Content="Total hours worked by employees" HorizontalAlignment="Left" Foreground="White" Margin="605,471,0,0" VerticalAlignment="Top" FontSize="16" FontWeight="Bold" Grid.Column="1" Height="35" Width="306"/>
                                <Label x:Name="lblHoursWorkedForPeriod" Content="Label" Foreground="White" HorizontalAlignment="Left" Margin="1027,471,0,0" VerticalAlignment="Top" FontSize="16" RenderTransformOrigin="1.904,0.613" Grid.Column="1" Height="35" Width="56"/>
                                <Label Content="Attendance rate" HorizontalAlignment="Left" Foreground="White" Margin="39,471,0,0" VerticalAlignment="Top" FontSize="16" FontWeight="Bold" Grid.Column="1" Height="35" Width="196"/>
                                <Label x:Name="lblHoursWorkedForPeriod_Copy" Content="Label" Foreground="White" HorizontalAlignment="Left" Margin="444,471,0,0" VerticalAlignment="Top" FontSize="16" RenderTransformOrigin="1.904,0.613" Grid.Column="1" Height="35" Width="55"/>
                                <Label Content="People present on" HorizontalAlignment="Left" Foreground="White" Margin="39,537,0,0" VerticalAlignment="Top" FontSize="16" FontWeight="Bold" Grid.Column="1" Height="35" Width="196"/>
                                <Label x:Name="lbDate1" Content="date" HorizontalAlignment="Left" Foreground="White" Margin="197,537,0,0" VerticalAlignment="Top" FontSize="16" RenderTransformOrigin="1.017,0.533" Grid.Column="1" Height="35" Width="210"/>
                                <Label x:Name="lbPeoplePresent" Content="Label" HorizontalAlignment="Left" Foreground="White" Margin="444,537,0,0" VerticalAlignment="Top" FontSize="16" RenderTransformOrigin="1.904,0.613" Grid.Column="1" Height="35" Width="55"/>
                                <Label Content="Attendance rate" HorizontalAlignment="Left"  Foreground="White" Margin="608,537,0,0" VerticalAlignment="Top" FontSize="16" FontWeight="Bold" Grid.Column="1" Height="35" Width="196"/>
                                <Label x:Name="lblHoursWorkedForPeriod_Copy1" Content="Label" Foreground="White" HorizontalAlignment="Left" Margin="1027,537,0,0" VerticalAlignment="Top" FontSize="16" RenderTransformOrigin="1.904,0.613" Grid.Column="1" Height="35" Width="55"/>
                                <Label Content="People present on" HorizontalAlignment="Left" Foreground="White" Margin="608,599,0,0" VerticalAlignment="Top" FontSize="16" FontWeight="Bold" Grid.Column="1" Height="35" Width="196"/>
                                <Label x:Name="lbDate1_Copy" Content="date" HorizontalAlignment="Left" Foreground="White" Margin="768,599,0,0" VerticalAlignment="Top" FontSize="16" RenderTransformOrigin="1.904,0.613" Grid.Column="1" Height="35" Width="128"/>
                                <Label x:Name="lbPeoplePresent_Copy" Content="Label" HorizontalAlignment="Left" Foreground="White" Margin="1027,599,0,0" VerticalAlignment="Top" FontSize="16" RenderTransformOrigin="1.904,0.613" Grid.Column="1" Height="35" Width="55"/>
                                <Label x:Name="lbDate1_Copy1" Content="date" HorizontalAlignment="Left" Foreground="White" Margin="887,599,0,0" VerticalAlignment="Top" FontSize="16" RenderTransformOrigin="1.904,0.613" Grid.Column="1" Height="35" Width="125"/>
                            </Grid>
                        </TabItem>
                        <TabItem Header="Products">
                            <Grid Background="#11589b" >
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="0*"/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <Label Content="Product statistics" Foreground="White" Grid.Column="1" HorizontalAlignment="Left" Margin="82,30,0,0" VerticalAlignment="Top" FontSize="24" FontWeight="Bold"/>
                                <Label Content="Average amount per purchase:" Foreground="White" Grid.Column="1" HorizontalAlignment="Left" Margin="82,162,0,0" VerticalAlignment="Top" FontSize="16" FontWeight="Bold"/>
                                <Label x:Name="lblAvgAmountPerPurchase" Content="Label" Foreground="White" Grid.Column="1" HorizontalAlignment="Left" Margin="335,162,0,0" VerticalAlignment="Top" FontSize="16" RenderTransformOrigin="2.16,0.661"/>
                                <ComboBox x:Name="cmbProducts" Grid.Column="1" HorizontalAlignment="Left" Margin="82,113,0,0" VerticalAlignment="Top" Width="213"/>
                                <Button x:Name="btnGetProductStat" Content="Proceed" Grid.Column="1" HorizontalAlignment="Left" Margin="335,113,0,0" VerticalAlignment="Top" Width="125" Height="22" Click="btnGetProductStat_Click_1" />
                                <Label x:Name="lbl" Content="Total profit per product" Foreground="White" Grid.Column="1" HorizontalAlignment="Left" Margin="82,213,0,0" VerticalAlignment="Top" FontSize="16" FontWeight="Bold"/>
                                <Label x:Name="lblTotalProfit" Content="Label" Foreground="White" Grid.Column="1" HorizontalAlignment="Left" Margin="335,213,0,0" VerticalAlignment="Top" FontSize="16" RenderTransformOrigin="2.16,0.661"/>
                                <Label Content="Total sales per product" Foreground="White" Grid.Column="1" HorizontalAlignment="Left" Margin="82,260,0,0" VerticalAlignment="Top" FontSize="16" FontWeight="Bold"/>
                                <Label x:Name="lblTotalSalesPerProduct" Foreground="White" Content="Label" Grid.Column="1" HorizontalAlignment="Left" Margin="335,260,0,0" VerticalAlignment="Top" FontSize="16" RenderTransformOrigin="2.16,0.661"/>
                            </Grid>


                        </TabItem>
                        <TabItem Header="Departments">
                            <Grid Background="#11589b" >
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="0*"/>
                                    <ColumnDefinition Width="101*"/>
                                    <ColumnDefinition Width="1023*"/>
                                </Grid.ColumnDefinitions>
                                <ComboBox x:Name="cmbDepartments" HorizontalAlignment="Left" Margin="87,169,0,0" VerticalAlignment="Top" Width="433" Grid.Column="1" Height="25" Grid.ColumnSpan="2"/>
                                <Button x:Name="btnGetPRoductStat" Content="Display" Grid.Column="2" HorizontalAlignment="Left" Margin="125.328,221,0,0" VerticalAlignment="Top" Width="161" Height="51" Click="btnGetPRoductStat_Click"/>
                                <Label x:Name="lblTotalProfitProduct" Foreground="White"  Content="Label" Grid.Column="2" HorizontalAlignment="Left" Margin="315,347,0,0" VerticalAlignment="Top" FontSize="16" RenderTransformOrigin="2.16,0.661" Height="36" Width="93"/>
                                <Label Content="Total sales per department" Foreground="White"  HorizontalAlignment="Left" Margin="547.328,347,0,0" VerticalAlignment="Top" FontSize="16" FontWeight="Bold" Grid.Column="2" Height="36" Width="210"/>
